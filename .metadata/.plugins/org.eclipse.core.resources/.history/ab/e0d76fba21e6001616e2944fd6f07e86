package engineTest;

import util.Window;

public class FontManager extends Window {
	
	private static FontManager inctance;
	
	private FontManager(){
		super("Font Manager", 500, 500);
	}
	
	public static void Open(boolean viwe) {
		if (inctance == null)
			inctance = new FontManager();
		else
			window.setVisible(viwe);
	}
	
	@Override
	public void Init() {
		super.Init();
	}
	

}
